; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "ReactGDComponent",
"language": "GDScript",
"path": "res://addons/react-gd/classes/ReactGDComponent.gd"
}, {
"base": "Node",
"class": "ReactGDDictionaryMethods",
"language": "GDScript",
"path": "res://addons/react-gd/utility/ReactGDDictionaryMethods.gd"
}, {
"base": "Object",
"class": "ReactGDTransition",
"language": "GDScript",
"path": "res://addons/react-gd/classes/ReactGDTransition.gd"
} ]
_global_script_class_icons={
"ReactGDComponent": "",
"ReactGDDictionaryMethods": "",
"ReactGDTransition": ""
}

[application]

config/name="ReactGD"
config/icon="res://icon.png"

[editor_plugins]

enabled=PoolStringArray(  )

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
2d/snapping/use_gpu_pixel_snap=true
environment/default_environment="res://default_env.tres"
